function BuildReact()
{
    docker exec $Repository$Project bash -c "Hash | xargs echo > /react/hash"
    docker exec $Repository$Project bash -c "cd /$Repository/$Project && npm run build"

    docker cp $Repository$Project:/$Repository/$Project/dist/. /Build
    docker cp $Repository$Project:/react/hash  /Build

    cd /Build
    zip -r panel.zip assets Fonts hash images index.html

    # rm -rf /Build/public/Fonts
}
